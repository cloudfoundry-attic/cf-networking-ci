#!/bin/bash -ex

cd cli-plugin-fork
git checkout update-networking-plugin
echo "Update network-policy plugin

This PR was automatically generated by the CF Networking CI when a new networking release was cut.

This can be merged as soon as the latest cf-networking release is published (https://github.com/cloudfoundry-incubator/cf-networking-release/releases)
" >> /tmp/pr-message
hub pull-request -F /tmp/pr-message -b cloudfoundry-incubator/cli-plugin-repo:master -h cf-container-networking/cli-plugin-repo:update-networking-plugin
